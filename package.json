{
  "name": "starter-blog",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "build": "contentlayer build && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node -r esbuild-register ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mdx-js/runtime": "1.6.22",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.13",
    "@next/bundle-analyzer": "13.1.6",
    "@next/font": "13.1.6",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vercel/analytics": "0.1.11",
    "autoprefixer": "^10.4.13",
    "contentlayer": "^0.3.4",
    "esbuild": "0.17.11",
    "github-slugger": "^1.4.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "lodash": "4.17.21",
    "matter-attractors": "0.1.6",
    "matter-js": "0.14.0",
    "mdx-bundler": "^9.0.0",
    "next": "13.1.6",
    "next-contentlayer": "^0.3.4",
    "next-mdx-remote": "4.4.1",
    "next-themes": "^0.2.0",
    "pliny": "0.0.10",
    "postcss": "^8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf": "6.2.2",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-citation": "^0.5.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "tailwindcss": "^3.2.2",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@types/lodash": "4.14.197",
    "@types/matter-js": "0.14.0",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "esbuild-register": "3.4.2",
    "eslint": "^8.23.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^8.0.0",
    "js-yaml": "4.1.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
